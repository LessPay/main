<form action class=" form form-inline" ng-submit="activeVoucher()">
	<legend>Activate voucher</legend>


    

    <div class="form-group">
    	<div class="input-group input-group-lg">
    		<div class="input-group-addon">
    			<i class="fa fa-qrcode"></i>
    		</div>
    		<input type="text" class="form-control" placeholder="Input voucher code" ng-model="data.vid">
    		<div class="input-group-btn">
    			<button class="btn btn-info " ng-click="activate()">
					<strong>
						Activate
					</strong>
				</button>

    		</div>    		
    	</div>
		
	</div>


    

	
</form>